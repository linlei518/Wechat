/*结构样式*/
#header{ background:#1161be; color:#fff; height:45px; position:fixed; top:0; left:0; width:100%; z-index:8; box-shadow:0 0 8px rgba(0,0,0,0.5)}
#header .logoField{ width:218px; float:left; height:100%;}
#header .logoField .logo{ display:block; padding:2px 0 0 8px; font-size:16px; line-height:40px; letter-spacing:1px;}
#header .logoField img{ vertical-align:middle;}

#header .userField{ float:right; padding:0 12px 0 0; position:relative; z-index:2;}
#header .userField p{ font-size:14px; line-height:45px;}
#header .userField a{ color:#fff;}

#header .titleField{ margin-left:218px; padding:0 0 0 25px; position:relative;}
#header .titleField h1{ color:#fff; font-size:14px; line-height:45px;}
#header .titleField h1 a{ color:#fff;}
#header .titleField ul{ position:absolute; top:43px; background:#fff; left:20px; padding:5px 5px 10px; border:2px solid #1161be; display:none; width:220px;}
#header .titleField ul li{ line-height:20px; border-bottom:1px solid #f1f1f1;}
#header .titleField ul li a{ display:block; padding:8px 10px 8px 5px; font-size:14px;}
#header .titleField ul li a:hover{ color:#1161be;}

#header .userField ul{ position:absolute; top:43px; background:#fff; right:18px; padding:5px 5px 10px; border:2px solid #1161be; display:none; width:140px;}
#header .userField ul li{ line-height:20px; border-bottom:1px solid #f1f1f1;}
#header .userField ul li a{ display:block; padding:5px 5px 5px 10px; color:#000; text-align:right;}
#header .userField ul li a:hover{ color:#1161be;}
#header .userField ul li em{ color:#e20000; font-weight:bold;}



#mainNav{ width:217px; position:absolute; left:0; top:0; min-height:100%; border-right:1px solid #d4d4d4;}
#mainNav ul{ width:100%; padding:83px 0 0;}
#mainNav ul li{ padding:0 18px;}
#mainNav ul li a{ display:block; padding:9px 0; line-height:24px; color:#222; font-size:14px; border-bottom:1px solid #ececec;}
#mainNav ul li a:hover{ color:#44b549; text-decoration:none;}
#mainNav ul li.current{ padding:0;}
#mainNav ul li.current a{ color:#fff; background:#1161be; padding:8px 18px;}

#mainNav ul li ul{ display:none; height:0; overflow:hidden; padding:0 0 5px; width:auto; border-bottom:1px solid #ececec;}
#mainNav ul li.current ul{ display:block; height:auto; margin:0 18px;}
#mainNav ul li ul li{ padding:0;}
#mainNav ul li ul li a{ padding:9px 0 9px 28px; border:0;}
#mainNav ul li.current ul li a{ background:none; color:#222; padding:9px 0 9px 28px;}
#mainNav ul li ul li a:hover{ color:#44b549;}
#mainNav ul li ul li.current a{ color:#1161be;}

#mainNav ul li.current ul li ul{ display:none; height:0; overflow:hidden; padding:0 0 5px; margin:0; border:0;}
#mainNav ul li.current ul li.current ul{ display:block; height:auto;}
#mainNav ul li ul li.current ul li{ background:none;}
#mainNav ul li ul li.current ul li a{ background:none; padding:8px 0 8px 28px; color:#666}
#mainNav ul li ul li.current ul li a:hover{ color:#44b549;}
#mainNav ul li ul li.current ul li.current a{ color:#1161be;}

.help #mainNav{ border:0;}
.help #mainNav ul{padding:83px 0 40px;}
.help #mainNav ul li a{ padding:9px 18px;}
.help #mainNav ul li.current{ padding:0 18px;}
.help #mainNav ul li.current a{background:#44b549; border-radius:4px; padding:9px 18px;}
.help #mainNav ul li.current a i{ display:none;}
.help #mainNav ul li ul{ padding:0 0 5px;}
.help #mainNav ul li.current ul{ margin:0;}
.help #mainNav ul li.current ul li a{ background:none; }
.help #mainNav ul li.current ul li.current a{ color:#44b549; padding:9px 0;}
.help #main{ border:0;}


#main{ position:absolute; left:217px; right:0; top:45px; padding:38px 0 0; border-left:1px solid #d4d4d4;}

#salesHeader{ position:absolute; left:0; right:0; top:0; height:40px; background:#0b50a1; z-index:6;}
#salesHeader .navField{ float:left; height:100%;}
#salesHeader .navField ul{ height:100%; text-align:left;}
#salesHeader .navField ul li{ display:inline-block; padding:0 20px; line-height:40px; height:100%; font-size:14px; color:#fff;}
#salesHeader .navField ul li a{ color:#fff;}
#salesHeader .navField ul li.current{ background:#f3f7fb; color:#0b50a1;}
#salesHeader .navField ul li.current a{ color:#0b50a1;}


#salesHeader .userField{ float:right; height:100%; position:relative; padding:0 0 0 8px;}
#salesHeader .userField p{ height:100%; color:#fff; line-height:40px; cursor:pointer;}
#salesHeader .userField p span{ display:inline-block; height:24px; width:24px; margin:8px; vertical-align:middle; overflow:hidden;}
#salesHeader .userField p span img{ display:block; width:100%; height:auto;}
#salesHeader .userField p em{ font-size:14px; color:#fff;}

#salesHeader .userField ul{ position:absolute; top:38px; background:#fff; right:0px; padding:5px 5px 10px; border:2px solid #1161be; display:none; width:180px;}
#salesHeader .userField ul li{ line-height:20px; border-bottom:1px solid #f1f1f1; text-align:right; color:#999;}
#salesHeader .userField ul li span{ display:block; padding:5px 0; text-align:right;}
#salesHeader .userField ul li a{ display:block; padding:5px 0; color:#000; text-align:right;}
#salesHeader .userField ul li a:hover{ color:#1161be;}
#salesHeader .userField ul li em{ color:#e20000; font-weight:bold;}

#salesHeader .btns{ float:right; height:100%; border-left:1px solid #295fa1;}
#salesHeader .btns .btn{ height:100%; width:47px; border-right:1px solid #295fa1; background:none; text-align:center;}
#salesHeader .btns .btn.messageBtn{ position:relative;}
#salesHeader .btns .btn.messageBtn i{ margin:0;}
#salesHeader .btns .btn.messageBtn sup{ position:absolute; top:1px; right:1px;}


#salesMain{ position:absolute; left:0; right:0; top:40px; bottom:0; background:#fff; overflow:auto;}
#salesMain.inputList{ padding:20px 0 0; background:#fff;}

/*弹框页面结构*/
.bombbox{ background:#fff; height:100%;}
