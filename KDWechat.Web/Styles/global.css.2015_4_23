.listPanel_02 dl.appShow{ }
.listPanel_02 dl.appShow dd{ border:1px solid #e6e7ec; background:#fff; padding:10px;}
.listPanel_02 dl.appShow dd .img{ float:left; width:100px; height:100px;}
.listPanel_02 dl.appShow dd .img span{ display:block; width:100%; height:100%; overflow: hidden;}
.listPanel_02 dl.appShow dd .img img{ display:block; width:100%; height:auto;}
.listPanel_02 dl.appShow dd .info{ margin:0 0 0 120px;}
.listPanel_02 dl.appShow dd .info h2{ color:#000;}
.listPanel_02 dl.appShow dd .info p{ color:#999;}
.listPanel_02 dl.appShow dd .info:after{ content: "."; display: block; height: 0; clear: both; visibility: hidden; overflow:hidden;}

.listPanel_02 .article, .listPanel_02 .link, .listPanel_02 .app{ display:none;}
.listPanel_02 .active{ display:block;}

.listPanel_02 .fileAera .txt{ display:none;}

.btnPanel_02{ padding:0 0 40px; margin:0 0 0 20px; width:1010px; text-align:right; font-size:0;}
.btnPanel_02 .btn{ margin:0 0 0 10px;}











